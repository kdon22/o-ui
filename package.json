  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:https": "node scripts/dev-https.js",
    "dev:all": "concurrently \"next dev\" \"npm run pyright-server\"",
    "dev:pyright": "npm run pyright-server & sleep 5 && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "fresh-start": "chmod +x scripts/fresh-start.sh && ./scripts/fresh-start.sh",
    "reset-browser": "open http://localhost:3000/clear-browser-data.html",
    "generate:entity": "ts-node scripts/generate-entity.ts",
    "restart": "next build && next start",
    "pyright-server": "cd ../pyright-server && chmod +x start-server.sh && ./start-server.sh",
    "pyright-server:debug": "cd ../pyright-server && chmod +x start-server.sh && DEBUG=1 ./start-server.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:loop": "jest loop-parameter.test.ts",
    "test:loop-direct": "npx ts-node --compiler-options '{\"module\":\"CommonJS\"}' src/lib/editor/unified-monaco-system/completion/test-runner.ts",
    "test:loop-quick": "npx ts-node --compiler-options '{\"module\":\"CommonJS\"}' src/lib/editor/unified-monaco-system/completion/test-runner.ts quick",
    "test:loop-full": "npx ts-node --compiler-options '{\"module\":\"CommonJS\"}' src/lib/editor/unified-monaco-system/completion/test-runner.ts full",
    "test:loop-user": "npx ts-node --compiler-options '{\"module\":\"CommonJS\"}' src/lib/editor/unified-monaco-system/completion/test-runner.ts user"
  },
